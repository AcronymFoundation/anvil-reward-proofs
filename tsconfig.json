{
  "compilerOptions": {
    "target": "es2022",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "outDir": "build"
  },
  "include": [
    "src/**/*",
  ],
  "exclude": [
    "node_modules",
  ]
}
